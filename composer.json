{
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-simplexml": "*",
        "ext-zip": "*",
        "bitrix24/b24phpsdk": "^1.8",
        "symfony/console": "~7.3.0",
        "symfony/dotenv": "~7.3.0",
        "symfony/flex": "^2.10",
        "symfony/framework-bundle": "~7.3.0",
        "symfony/monolog-bundle": "^3.10",
        "symfony/runtime": "~7.3.0",
        "symfony/twig-bundle": "~7.3.0",
        "symfony/yaml": "~7.3.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.48.2",
        "friendsofphp/php-cs-fixer": "^3.90.0",
        "phpstan/phpstan": "^2.1.32",
        "phpyh/coding-standard": "^2.6.3",
        "rector/rector": "^2.2.8"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "cs:check": "php-cs-fixer check",
        "cs:fix": "php-cs-fixer fix",
        "phpstan": "phpstan",
        "rector:check": "rector process --dry-run",
        "rector:fix": "rector process"
    },
    "scripts-descriptions": {
        "cs:check": "Check coding standards",
        "cs:fix": "Apply coding standards",
        "phpstan": "Run code analysis",
        "rector:check": "Check rector rules",
        "rector:fix": "Apply rector rules"
    }
}
